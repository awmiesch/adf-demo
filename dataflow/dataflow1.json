{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_airtable_country_codes",
						"type": "DatasetReference"
					},
					"name": "countryCodesRestSource"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "ls_azure_blob_storage",
						"type": "LinkedServiceReference"
					},
					"name": "countryCodesDataBricks"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          body as (offset as string, records as (createdTime as string, fields as (Code as string, Name as string), id as string)[]),",
				"          headers as [string,string]",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     httpMethod: 'GET',",
				"     timeout: 30,",
				"     requestInterval: 0,",
				"     paginationRules: ['supportRFC5988' -> 'true'],",
				"     responseFormat: ['type' -> 'json', 'documentForm' -> 'arrayOfDocuments']) ~> countryCodesRestSource",
				"countryCodesRestSource sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     container: 'datafiles',",
				"     filePattern:'',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> countryCodesDataBricks"
			]
		}
	}
}