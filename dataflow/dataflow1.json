{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_airtable_country_codes",
						"type": "DatasetReference"
					},
					"name": "countryCodesRestSource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "country_code_records",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"rejectedDataLinkedService": {
						"referenceName": "ls_azure_blob_storage",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          body as (offset as string, records as (createdTime as string, fields as (Code as string, Name as string), id as string)[]),",
				"          headers as [string,string]",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     httpMethod: 'GET',",
				"     timeout: 30,",
				"     paginationRules: ['supportRFC5988' -> 'true'],",
				"     responseFormat: ['type' -> 'json', 'documentForm' -> 'arrayOfDocuments']) ~> countryCodesRestSource",
				"countryCodesRestSource sink(allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     mapColumn(",
				"          code = body.records[1].fields.Code,",
				"          name = body.records[1].fields.Name",
				"     )) ~> sink1"
			]
		}
	}
}